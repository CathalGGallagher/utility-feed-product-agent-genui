# Feed Products AI Agent Configuration
# Copy this file to .env and fill in your values

# Google Gemini API Key (required for AI-powered queries)
# Get your key at: https://ai.google.dev/
GOOGLE_API_KEY=your_api_key_here

# Gemini Model to use (optional, defaults to gemini-1.5-flash)
# Options: gemini-1.5-flash, gemini-1.5-pro, gemini-pro
GEMINI_MODEL=gemini-1.5-flash

# API Server Configuration (optional)
API_HOST=0.0.0.0
API_PORT=8000
